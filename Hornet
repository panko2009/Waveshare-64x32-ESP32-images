#include <ESP32-HUB75-MatrixPanel-I2S-DMA.h>

// ---------------- Panel config ----------------
#define PANEL_RES_X 64
#define PANEL_RES_Y 32
#define PANEL_CHAIN 1

HUB75_I2S_CFG cfg(PANEL_RES_X, PANEL_RES_Y, PANEL_CHAIN);
MatrixPanel_I2S_DMA* matrix = nullptr;

void setup() {
  // Pin mapping – must match your wiring
  matrix = new MatrixPanel_I2S_DMA(cfg);
  matrix->begin();
  matrix->setBrightness8(50);   // 0–255
  matrix->fillScreen(0);        // clear everything
}

void loop() {
  matrix->fillScreen(0);        // clear each frame

// === COPY THIS INTO YOUR ESP IDE ===
  matrix->drawPixelRGB888(0, 0, 255, 255, 255);
